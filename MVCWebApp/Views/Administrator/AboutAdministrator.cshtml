<h3>Personal info</h3>

<html>

<head>

</head>

<body>
    <form class="form-horizontal" role="form">
        <label for="KorisnickoIme" class="text-info">Administrator: @ViewBag.korisnicko_ime </label>
        <div class="form-group">
            <label class="col-lg-3 control-label">First name: </label>
            <div class="col-lg-8">
                <input class="form-control" type="text" value="@ViewBag.AdminPodaci.Ime">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 control-label">Last name: </label>
            <div class="col-lg-8">
                <input class="form-control" type="text" value="@ViewBag.AdminPodaci.Prezime">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 control-label">Username: </label>
            <div class="col-lg-8">
                <input class="form-control" type="text" value="@ViewBag.AdminPodaci.KorisnickoIme">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 control-label">Password: </label>
            <div class="col-lg-8">
                <input class="form-control" type="text" value="@ViewBag.AdminPodaci.Lozinka">
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 control-label"></label>
            <div class="col-lg-8">
                <input class="btn btn-danger" type="submit" value="Save" name="Save">
            </div>
        </div>
    </form>
</body>

</html>
